<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="facilities_travels_action" model="ir.actions.act_window">
            <field name="name">Facilities</field>
            <field name="res_model">facilities.travels</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="facilities_travels_view_tree" model="ir.ui.view">
            <field name="name">facilities.travels.tree</field>
            <field name="model">facilities.travels</field>
            <field name="arch" type="xml">
                <tree string="facilities_travels">
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>