<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>
        <record id="location_travels_action" model="ir.actions.act_window">
            <field name="name">Location</field>
            <field name="res_model">location.travels</field>
            <field name="view_mode">tree,form</field>
        </record>

        <record id="location_travels_view_tree" model="ir.ui.view">
            <field name="name">location_travels.tree</field>
            <field name="model">location.travels</field>
            <field name="arch" type="xml">
                <tree string="location_travels">
                    <field name="location"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>